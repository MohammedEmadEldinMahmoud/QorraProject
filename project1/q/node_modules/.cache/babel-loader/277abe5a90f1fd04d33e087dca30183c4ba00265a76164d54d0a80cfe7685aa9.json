ا المقال، سنستكشف فوائد القراءة اليومية وكيف يمكن أن تؤثر بشكل إيجابي على حياتنا.\"\n      }]\n    };\n  },\n  methods: {\n    filter(result) {\n      if (result === \"الكل\") {\n        this.searchresult = this.articles;\n      } else {\n        this.searchresult = this.articles.filter(article => article.article_result === result);\n      }\n\n      // Update the select state and topic\n      this.select = result === \"الكل\";\n      this.result = result;\n    }\n  }\n};","map":{"version":3,"names":["Swiper","SwiperSlide","Pagination","components","setup","modules","results","select","result","data","searchresult","articles","articleURL","require","articleTitle","articleContent","methods","filter","article","article_result"],"sources":["/Users/mohammed/Desktop/project1/q/src/views/searchResult.vue"],"sourcesContent":["<template>\n    <div class=\"container\">\n        <div class=\"sidebar\">\n            <h1>الاقسام</h1>\n            <p v-for=\"result in results\"  @click=\"filter(result)\">\n                {{ result }}\n            </p>\n        </div>\n        <h1 class=\"h1_topic\">نتائج البحث عن : {{ this.$route.params.result }}</h1>\n        <div class=\"row\" v-for=\"article in articles\">\n            <div class=\"col-9\">\n                <img :src=\"article.articleURL\" alt=\"\" />\n                <div class=\"about_article\">\n                    <p class=\"writer\">كتب بواسطة : أحمد السيد</p>\n                    <h1 class=\"article_title1\">{{ article.articleTitle }}</h1>\n                    <p class=\"part_of_article\">{{ article.articleContent }}</p>\n                    <div class=\"time_and_topic\">\n                        <div class=\"topic\" id=\"topic\">{{ article.result }}</div>\n                        <!-- <div class=\"time\">{{ article.article_from }}.{{ article.article_time_read }}</div> -->\n                    </div>\n                </div>\n            </div>\n        </div>\n\n    </div>\n</template>\n\n\n<script>\nimport { Swiper, S