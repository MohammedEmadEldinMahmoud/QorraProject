ياتنا.\"},\n                {articleURL:require(\"../assets/Rectangle11.png\") ,result:\"أدب\",articleTitle:\"فوائد القراءة اليومية\",articleContent:\"لقراءة هي نشاط ممتع ومفيد يعزز التعلم والتنمية الشخصية. إن القراءة اليومية لها تأثير كبير على حياتنا وتعزز الذكاء والتفكير الإبداعي. في هذا المقال، سنستكشف فوائد القراءة اليومية وكيف يمكن أن تؤثر بشكل إيجابي على حياتنا.\"},\n            ],\n        };\n    },\n    methods: {\n        // Method to filter the articles by topic\n        filter(result) {\n            if (result === \"الكل\") {\n                // Show all articles if \"الكل\" is selected\n                this.searchresult = this.articles;\n            } else {\n                // Filter articles based on the selected topic\n                this.search_results = this.articles.filter(\n                    (article) => article.article_result === result\n                );\n            }\n\n            // Update the select state and topic\n            this.select = result === \"الكل\";\n            this.result = result;\n        },\n    },\n    \n};\n</script>"],"mappings":";;EACSA,KAAK,EAAC;AAAW;;EACbA,KAAK,EAAC;AAAS;gCAChBC,mBAAA,CAAgB,YAAZ,SAAO;;;EAKXD,KAAK,EAAC;AAAU;;EACfA,KAAK,EAAC;AAAK;;EACPA,KAAK,EAAC;AAAO;;;EAETA,KAAK,EAAC;AAAe;iCACtBC,mBAAA,CAA6C;EAA1CD,KAAK,EAAC;AAAQ,GAAC,yBAAuB;;EACrCA,KAAK,EAAC;AAAgB;;EACvBA,KAAK,EAAC;AAAiB;;EACrBA,KAAK,EAAC;AAAgB;;EAClBA,KAAK,EAAC,OAAO;EAACE,EAAE,EAAC;;;uBAhB1CC,mBAAA,CAuBM,OAvBNC,UAuBM,GAtBFH,mBAAA,CAKM,OALNI,UAKM,GAJFC,UAAgB,G,kBAChBH,mBAAA,CAEII,SAAA,QAAAC,WAAA,CAFgBC,MAAA,CAAAC,OAAO,EAAjBC,MAAM;yBAAhBR,mBAAA,CAEI;MAF2BS,OAAK,EAAAC,MAAA,IAAEC,QAAA,CAAAC,MAAM,CAACJ,MAAM;wBAC5CA,MAAM,wBAAAK,UAAA;sCAGjBf,mBAAA,CAA0E,MAA1EgB,UAA0E,EAArD,mBAAiB,GAAAC,gBAAA,MAAQC,MAAM,CAACC,MAAM,CAACT,MAAM,mB,kBAClER,mBAAA,CAaMI,SAAA,QAAAC,WAAA,CAb6Ba,KAAA,CAAAC,QAAQ,EAAnBC,OAAO;yBAA/BpB,mBAAA,CAaM,OAbNqB,UAaM,GAZFvB,mBAAA,CAWM,OAXNwB,UAWM,GAVFxB,mBAAA,CAAwC;MAAlCyB,GAAG,EAAEH,OAAO,CAACI,UAAU;MAAEC,GAAG,EAAC;yCACnC3B,mBAAA,CAQM,OARN