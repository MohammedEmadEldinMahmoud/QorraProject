{"ast":null,"code":"import { Swiper, SwiperSlide } from \"swiper/vue\";\nimport \"swiper/css\";\nimport \"swiper/css/pagination\";\nimport { Pagination } from \"swiper\";\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n  setup() {\n    return {\n      modules: [Pagination],\n      topics: [\"الكل\", \"ادب\", \"فلسفه\", \"لغه\", \"موسيقي\", \"فن\"],\n      select: true,\n      topresic: \"الكل\"\n    };\n  },\n  data() {\n    return {\n      search_results: []\n    };\n  },\n  methods: {\n    // Method to filter the articles by topic\n    filter(result) {\n      if (result === \"الكل\") {\n        // Show all articles if \"الكل\" is selected\n        this.search_results = this.articles;\n      } else {\n        // Filter articles based on the selected topic\n        this.search_results = this.articles.filter(article => article.article_result === result);\n      }\n\n      // Update the select state and topic\n      this.select = result === \"الكل\";\n      this.result = result;\n    }\n  }\n};","map":{"version":3,"names":["Swiper","SwiperSlide","Pagination","components","setup","modules","topics","select","topresic","data","search_results","methods","filter","result","articles","article","article_result"],"sources":["/Users/mohammed/Desktop/project1/q/src/views/searchResult.vue"],"sourcesContent":["<template>\n    <div class=\"container\">\n        <div class=\"sidebar\">\n            <h1>الاقسام</h1>\n            <p v-for=\"result in results\"  @click=\"filter(result)\">\n                {{ result }}\n            </p>\n        </div>\n        <h1 class=\"h1_topic\">نتائج البحث عن : {{ this.$route.params.topic }}</h1>\n        <div class=\"row\" v-for=\"article in search_results\" :key=\"article.id\">\n            <div class=\"col-9\">\n                <img :src=\"article.article_img_url\" alt=\"\" />\n                <div class=\"about_article\">\n                    <p class=\"writer\">كتب بواسطة: {{ article.article_writer }}</p>\n                    <h1 class=\"article_title1\">{{ article.article_title }}</h1>\n                    <p class=\"part_of_article\">{{ article.part_of_article }}</p>\n                    <div class=\"time_and_topic\">\n                        <div class=\"topic\" id=\"topic\">{{ article.article_topic }}</div>\n                        <div class=\"time\">{{ article.article_from }}.{{ article.article_time_read }}</div>\n                    </div>\n                </div>\n            </div>\n        </div>\n\n    </div>\n</template>\n\n\n<script>\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\nimport \"swiper/css\";\nimport \"swiper/css/pagination\";\nimport { Pagination } from \"swiper\";\nexport default {\n    components: {\n        Swiper,\n        SwiperSlide,\n        \n    },\n    setup() {\n        return {\n            modules: [Pagination],\n            topics: [\"الكل\", \"ادب\", \"فلسفه\", \"لغه\", \"موسيقي\", \"فن\"],\n            select: true,\n            topresic: \"الكل\",\n        };\n    },\n    data() {\n        return {\n            search_results: [],\n        };\n    },\n    methods: {\n        // Method to filter the articles by topic\n        filter(result) {\n            if (result === \"الكل\") {\n                // Show all articles if \"الكل\" is selected\n                this.search_results = this.articles;\n            } else {\n                // Filter articles based on the selected topic\n                this.search_results = this.articles.filter(\n                    (article) => article.article_result === result\n                );\n            }\n\n            // Update the select state and topic\n            this.select = result === \"الكل\";\n            this.result = result;\n        },\n    },\n    \n};\n</script>"],"mappings":"AA6BA,SAASA,MAAM,EAAEC,WAAU,QAAS,YAAY;AAChD,OAAO,YAAY;AACnB,OAAO,uBAAuB;AAC9B,SAASC,UAAS,QAAS,QAAQ;AACnC,eAAe;EACXC,UAAU,EAAE;IACRH,MAAM;IACNC;EAEJ,CAAC;EACDG,KAAKA,CAAA,EAAG;IACJ,OAAO;MACHC,OAAO,EAAE,CAACH,UA